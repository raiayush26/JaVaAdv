abstract class A{ // Inecapculatuion
    void mymethod(){
        System.out.println(" In method");
    }
    abstract void yourmethod();
}
class B extends A{
    void yourmethod(){
        System.out.println("In your method");
    }
}
public class Demo3{
    public static void main (String args[]){
        //A obj =new A(); // A is abract class we can not create object from it
        B obj=new B();
        obj.yourmethod();
    }
}